{
  "name": "fynk",
  "version": "1.0.0",
  "description": "Fynk â€” reactive HTTP client with normalized cache, dedup, optimistic updates, and pluggable interceptors for React & Vue.",
  "type": "module",
  "main": "./dist-cjs/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "dist-cjs"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist-cjs/index.js",
      "default": "./dist/index.js"
    },
    "./react": {
      "types": "./dist/react/index.d.ts",
      "import": "./dist/react/index.js",
      "require": "./dist-cjs/react/index.js",
      "default": "./dist/react/index.js"
    },
    "./vue": {
      "types": "./dist/vue/index.d.ts",
      "import": "./dist/vue/index.js",
      "require": "./dist-cjs/vue/index.js",
      "default": "./dist/vue/index.js"
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.json && tsc -p tsconfig.cjs.json",
    "dev": "tsc -w -p tsconfig.json",
    "bench": "tsx benchmarks/runBench.ts",
    "clean": "rimraf dist dist-cjs",
    "prepublishOnly": "npm run clean && npm run build",
    "publish:dry": "npm publish --dry-run",
    "publish:beta": "npm publish --tag beta"
  },
  "keywords": [
    "http",
    "cache",
    "reactive",
    "vue",
    "react",
    "interceptor",
    "sse",
    "dedup"
  ],
  "author": "Fynk Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ljlm0402/fynk.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "react": "^19.0.0",
    "vue": "^3.4.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/node": "^20.11.30",
    "@types/react": "^19.2.2",
    "@types/vue": "^1.0.31",
    "axios": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6",
    "typescript": "^5.6.2",
    "vue": "^3.5.22"
  }
}
